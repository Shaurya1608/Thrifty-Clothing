{
  "name": "thrifty-clothings",
  "version": "1.0.0",
  "description": "Full-stack e-commerce website for THRIFTY CLOTHINGS",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "keywords": [
    "e-commerce",
    "clothing",
    "thrifty",
    "full-stack"
  ],
  "author": "THRIFTY CLOTHINGS",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "mongoose": "^8.18.0",
    "resend": "^6.0.1",
    "vercel": "^46.0.2"
  }
}
